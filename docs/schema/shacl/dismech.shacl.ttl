@prefix GENO: <http://purl.obolibrary.org/obo/GENO_> .
@prefix HP: <http://purl.obolibrary.org/obo/HP_> .
@prefix dismech: <https://w3id.org/monarch-initiative/dismech/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

linkml:Any a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass linkml:Any .

dismech:Assay a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path dismech:name ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path dismech:description ] ;
    sh:targetClass dismech:Assay .

dismech:DiseaseCollection a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class dismech:Disease ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path dismech:diseases ] ;
    sh:targetClass dismech:DiseaseCollection .

dismech:AnimalModel a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class dismech:EvidenceItem ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 8 ;
            sh:path dismech:evidence ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path dismech:category ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path dismech:description ],
        [ sh:datatype xsd:string ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path dismech:alleles ],
        [ sh:class dismech:GeneDescriptor ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path dismech:genes ],
        [ sh:datatype xsd:string ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path dismech:associated_phenotypes ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path dismech:genotype ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path dismech:species ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path dismech:background ] ;
    sh:targetClass dismech:AnimalModel .

dismech:Biochemical a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path dismech:context ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path dismech:subtype ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path dismech:notes ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path dismech:name ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path dismech:presence ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path dismech:specificity ],
        [ sh:class dismech:Descriptor ;
            sh:description "Ontology term for a biomarker (from NCIT, CHEBI, or LOINC)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path dismech:biomarker_term ],
        [ sh:class dismech:CellTypeDescriptor ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 9 ;
            sh:path dismech:cell_types ],
        [ sh:datatype xsd:string ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path dismech:synonyms ],
        [ sh:maxCount 1 ;
            sh:or ( [ sh:in ( HP:0040282 HP:0040283 HP:0040281 HP:0040284 HP:0040280 ) ] [ sh:nodeKind sh:Literal ] ) ;
            sh:order 5 ;
            sh:path dismech:frequency ],
        [ sh:class dismech:EvidenceItem ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path dismech:evidence ],
        [ sh:class dismech:AssayDescriptor ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 10 ;
            sh:path dismech:assays ] ;
    sh:targetClass dismech:Biochemical .

dismech:CellularComponentDescriptor a sh:NodeShape ;
    sh:closed true ;
    sh:description "A descriptor for cellular components, bindable to GO cellular component" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "The preferred human-readable term for this descriptor" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path dismech:preferred_term ],
        [ sh:datatype xsd:string ;
            sh:description "A description of the descriptor. This may typically be redundant with the `term` object, but the description is more human-readable and may be used to communicate nuances not captured by the rigid standardization of the term object." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path dismech:description ],
        [ sh:class dismech:Term ;
            sh:description "Optional GO cellular component term reference" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path dismech:term ],
        [ sh:class dismech:AnatomicalEntityDescriptor ;
            sh:description "Anatomical location where this entity/process occurs or procedure is performed" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path dismech:located_in ],
        [ sh:description "Laterality qualifier (left, right, or bilateral)" ;
            sh:in ( "LEFT" "RIGHT" "BILATERAL" ) ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path dismech:laterality ],
        [ sh:description "Directional or qualitative modifier for a descriptor (e.g., increased, decreased, abnormal)" ;
            sh:in ( "INCREASED" "DECREASED" "ABNORMAL" "DYSREGULATED" "ABSENT" ) ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path dismech:modifier ],
        [ sh:class dismech:Qualifier ;
            sh:description "List of predicate-value pairs for formal post-composition. Allows OWL-like expressivity with controlled predicates (e.g., RO relations) and values." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 6 ;
            sh:path dismech:qualifiers ] ;
    sh:targetClass dismech:CellularComponentDescriptor .

dismech:ChemicalEntityDescriptor a sh:NodeShape ;
    sh:closed true ;
    sh:description "A descriptor for chemical entities, bindable to CHEBI" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Laterality qualifier (left, right, or bilateral)" ;
            sh:in ( "LEFT" "RIGHT" "BILATERAL" ) ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path dismech:laterality ],
        [ sh:datatype xsd:string ;
            sh:description "The preferred human-readable term for this descriptor" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path dismech:preferred_term ],
        [ sh:class dismech:Term ;
            sh:description "Optional CHEBI chemical entity term reference" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path dismech:term ],
        [ sh:description "Directional or qualitative modifier for a descriptor (e.g., increased, decreased, abnormal)" ;
            sh:in ( "INCREASED" "DECREASED" "ABNORMAL" "DYSREGULATED" "ABSENT" ) ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path dismech:modifier ],
        [ sh:class dismech:AnatomicalEntityDescriptor ;
            sh:description "Anatomical location where this entity/process occurs or procedure is performed" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path dismech:located_in ],
        [ sh:datatype xsd:string ;
            sh:description "A description of the descriptor. This may typically be redundant with the `term` object, but the description is more human-readable and may be used to communicate nuances not captured by the rigid standardization of the term object." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path dismech:description ],
        [ sh:class dismech:Qualifier ;
            sh:description "List of predicate-value pairs for formal post-composition. Allows OWL-like expressivity with controlled predicates (e.g., RO relations) and values." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 6 ;
            sh:path dismech:qualifiers ] ;
    sh:targetClass dismech:ChemicalEntityDescriptor .

dismech:ComputationalModel a sh:NodeShape ;
    sh:closed true ;
    sh:description "A computational or in-silico model relevant to understanding disease mechanisms" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Software/toolbox for running the model (e.g., COBRApy, COBRA Toolbox)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path dismech:model_software ],
        [ sh:description "Associated publication (PMID)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path dismech:publication ],
        [ sh:datatype xsd:string ;
            sh:description "Identifier within the repository (e.g., Recon3D, BIOMD0000000123)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path dismech:model_id ],
        [ sh:datatype xsd:string ;
            sh:description "File format (e.g., SBML, MATLAB, JSON, ONNX)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path dismech:model_format ],
        [ sh:datatype xsd:anyURI ;
            sh:description "URL to model repository (GitHub, BiGG, VMH, BioModels)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path dismech:repository_url ],
        [ sh:class dismech:GeneDescriptor ;
            sh:description "Gene knockouts, reaction deletions, or parameter changes modeling the disease" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 6 ;
            sh:path dismech:perturbations ],
        [ sh:datatype xsd:string ;
            sh:description "Parent/base model this is derived from (e.g., Recon3D, Harvey 1.0)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path dismech:base_model ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path dismech:name ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path dismech:description ],
        [ sh:class dismech:EvidenceItem ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 10 ;
            sh:path dismech:evidence ],
        [ sh:description "Type of computational model" ;
            sh:in ( "GENOME_SCALE_METABOLIC" "FLUX_BALANCE_ANALYSIS" "KINETIC" "AGENT_BASED" "BOOLEAN_NETWORK" "PHYSIOLOGICAL" "DIGITAL_TWIN" "MACHINE_LEARNING" "PERTURBATION_PREDICTION" "FOUNDATION_MODEL" ) ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path dismech:model_type ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path dismech:notes ] ;
    sh:targetClass dismech:ComputationalModel .

dismech:CurationEvent a sh:NodeShape ;
    sh:closed true ;
    sh:description "A single curation event in the audit trail" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:dateTime ;
            sh:description "ISO 8601 timestamp of the curation event" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path dismech:curation_timestamp ],
        [ sh:datatype xsd:string ;
            sh:description "Model identifier used for curation (e.g., claude-opus-4-5-20251101)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path dismech:curation_model ],
        [ sh:description "Type of curation action performed" ;
            sh:in ( "CREATED" "EDITED" ) ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path dismech:curation_action ],
        [ sh:datatype xsd:string ;
            sh:description "Human-readable description of changes made" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path dismech:curation_description ] ;
    sh:targetClass dismech:CurationEvent .

dismech:Dataset a sh:NodeShape ;
    sh:closed true ;
    sh:description "A reference to a publicly available omics or phenotype dataset" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Title of the publication" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path dismech:title ],
        [ sh:class dismech:OrganismDescriptor ;
            sh:description "The organism from which samples were derived" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path dismech:organism ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 13 ;
            sh:path dismech:notes ],
        [ sh:class dismech:EvidenceItem ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 12 ;
            sh:path dismech:evidence ],
        [ sh:class dismech:SampleTypeDescriptor ;
            sh:description "Types of biological samples in the dataset" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path dismech:sample_types ],
        [ sh:description "Associated publication (PMID)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path dismech:publication ],
        [ sh:class dismech:GeneDescriptor ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 9 ;
            sh:path dismech:genes ],
        [ sh:description "The type of omics or other data in the dataset" ;
            sh:in ( "MICROARRAY" "BULK_RNA_SEQ" "SINGLE_CELL_RNA_SEQ" "SPATIAL_TRANSCRIPTOMICS" "METHYLATION" "CHIP_SEQ" "ATAC_SEQ" "PROTEOMICS" "METABOLOMICS" "GWAS" "WGS" "WES" "PHENOPACKETS" "VARIANT_DATABASE" ) ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path dismech:data_type ],
        [ sh:datatype xsd:string ;
            sh:description "A description of the dataset. This may typically be redundant with the `title` slot, but the description is more human-readable and may be used to communicate nuances not captured by the rigid standardization of the title slot." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path dismech:description ],
        [ sh:class dismech:ExposureDescriptor ;
            sh:description "Environmental exposures studied in the dataset" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 8 ;
            sh:path dismech:exposures ],
        [ sh:datatype xsd:integer ;
            sh:description "Total number of samples in the dataset" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path dismech:sample_count ],
        [ sh:datatype xsd:string ;
            sh:description "Sequencing or array platform used" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path dismech:platform ],
        [ sh:datatype xsd:string ;
            sh:description "Experimental conditions or disease states represented" ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path dismech:conditions ],
        [ sh:datatype xsd:anyURI ;
            sh:description "Dataset accession identifier as a CURIE (e.g., geo:GSE67472)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path dismech:accession ] ;
    sh:targetClass dismech:Dataset .

dismech:Diagnosis a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path dismech:description ],
        [ sh:class dismech:EvidenceItem ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path dismech:evidence ],
        [ sh:class dismech:TreatmentDescriptor ;
            sh:description "The MAXO term for this diagnostic procedure" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path dismech:diagnosis_term ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path dismech:notes ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path dismech:name ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path dismech:presence ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path dismech:markers ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path dismech:results ] ;
    sh:targetClass dismech:Diagnosis .

dismech:DifferentialDiagnosis a sh:NodeShape ;
    sh:closed true ;
    sh:description "A disease or condition that presents similarly to the focal disease and must be differentiated" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Key clinical, laboratory, imaging, or epidemiological features that help differentiate this condition from the focal disease" ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path dismech:distinguishing_features ],
        [ sh:class dismech:Phenotype ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path dismech:phenotypes ],
        [ sh:class dismech:DiseaseDescriptor ;
            sh:description "The MONDO disease term for this disease" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 6 ;
            sh:path dismech:disease_term ],
        [ sh:datatype xsd:string ;
            sh:description "Additional clinical notes or management considerations" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path dismech:notes ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path dismech:name ],
        [ sh:class dismech:EvidenceItem ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path dismech:evidence ],
        [ sh:datatype xsd:string ;
            sh:description "Clinical or mechanistic overlaps, shared presentations, and diagnostic considerations with the focal disease" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path dismech:description ] ;
    sh:targetClass dismech:DifferentialDiagnosis .

dismech:Disease a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class dismech:InfectiousAgent ;
            sh:nodeKind sh:IRI ;
            sh:order 18 ;
            sh:path dismech:infectious_agent ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path dismech:name ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path dismech:category ],
        [ sh:datatype xsd:string ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path dismech:parents ],
        [ sh:class dismech:Variant ;
            sh:nodeKind sh:IRI ;
            sh:order 14 ;
            sh:path dismech:variants ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 30 ;
            sh:path dismech:review_notes ],
        [ sh:class dismech:Transmission ;
            sh:nodeKind sh:IRI ;
            sh:order 19 ;
            sh:path dismech:transmission ],
        [ sh:class dismech:Pathophysiology ;
            sh:nodeKind sh:IRI ;
            sh:order 9 ;
            sh:path dismech:pathophysiology ],
        [ sh:class dismech:EpidemiologyInfo ;
            sh:nodeKind sh:IRI ;
            sh:order 21 ;
            sh:path dismech:epidemiology ],
        [ sh:class dismech:Genetic ;
            sh:nodeKind sh:IRI ;
            sh:order 13 ;
            sh:path dismech:genetic ],
        [ sh:class dismech:Dataset ;
            sh:description "Publicly available datasets relevant to disease research" ;
            sh:nodeKind sh:IRI ;
            sh:order 27 ;
            sh:path dismech:datasets ],
        [ sh:class dismech:Inheritance ;
            sh:nodeKind sh:IRI ;
            sh:order 25 ;
            sh:path dismech:inheritance ],
        [ sh:class dismech:Phenotype ;
            sh:nodeKind sh:IRI ;
            sh:order 10 ;
            sh:path dismech:phenotypes ],
        [ sh:class dismech:Diagnosis ;
            sh:nodeKind sh:IRI ;
            sh:order 22 ;
            sh:path dismech:diagnosis ],
        [ sh:datatype xsd:string ;
            sh:nodeKind sh:Literal ;
            sh:order 24 ;
            sh:path dismech:synonyms ],
        [ sh:class dismech:Prevalence ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 7 ;
            sh:path dismech:prevalence ],
        [ sh:class dismech:ComputationalModel ;
            sh:description "Computational models (metabolic, mechanistic, ML, digital twins) for this disease" ;
            sh:nodeKind sh:IRI ;
            sh:order 28 ;
            sh:path dismech:computational_models ],
        [ sh:class dismech:Subtype ;
            sh:nodeKind sh:IRI ;
            sh:order 6 ;
            sh:path dismech:has_subtypes ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 29 ;
            sh:path dismech:notes ],
        [ sh:class dismech:ProgressionInfo ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 8 ;
            sh:path dismech:progression ],
        [ sh:class dismech:CurationEvent ;
            sh:description "Audit trail of AI-assisted curation events" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 31 ;
            sh:path dismech:curation_history ],
        [ sh:class dismech:Environmental ;
            sh:nodeKind sh:IRI ;
            sh:order 15 ;
            sh:path dismech:environmental ],
        [ sh:class dismech:ModelingConsideration ;
            sh:nodeKind sh:IRI ;
            sh:order 20 ;
            sh:path dismech:modeling_considerations ],
        [ sh:class dismech:DiseaseDescriptor ;
            sh:description "The MONDO disease term for this disease" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path dismech:disease_term ],
        [ sh:class dismech:DifferentialDiagnosis ;
            sh:description "Differential diagnoses - similar diseases that must be ruled out" ;
            sh:nodeKind sh:IRI ;
            sh:order 23 ;
            sh:path dismech:differential_diagnoses ],
        [ sh:class dismech:Stage ;
            sh:nodeKind sh:IRI ;
            sh:order 12 ;
            sh:path dismech:stages ],
        [ sh:class dismech:PublicationReference ;
            sh:description "Top-level list of references with their key findings for this disease" ;
            sh:nodeKind sh:IRI ;
